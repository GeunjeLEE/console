{
  "name": "@spaceone/console-core-lib",
  "version": "1.0.22",
  "description": "SpaceONE Console Core Library",
  "scripts": {
    "build": "npm run babel && npm run tsc && cp -r dist/. . && rimraf src",
    "babel": "babel src --out-dir dist --extensions \".ts\" --source-maps inline --copy-files",
    "tsc": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "semantic-release": "SKIP_PREPARE_COMMIT_MSG=1 SKIP_COMMIT_MSG=1 semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spaceone-dev/console-core-lib.git"
  },
  "author": "SpaceONE",
  "license": "Apache-2.0",
  "homepage": "https://github.com/spaceone-dev/console-core-lib#readme",
  "dependencies": {
    "@spaceone/design-system": "^1.1.0-beta.94",
    "axios": "^0.21.1",
    "axios-auth-refresh": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/luxon": "^1.27.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "dayjs": "^1.10.6",
    "eslint": "^7.30.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.9",
    "semantic-release": "^17.4.4",
    "tsc-alias": "^1.3.7",
    "typescript": "^4.3.5"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
